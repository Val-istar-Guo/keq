{
  "name": "keq",
  "main": "lib/index.js",
  "module": "es/index.js",
  "version": "1.0.0",
  "license": "MIT",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "ava",
    "test:debug": "ava --verbose",
    "test:update": "ava -u",
    "report": "nyc --reporter=html --reporter=text-summary --skip-full ava",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "ci": "git-cz",
    "ca": "git add -A && git-cz -av",
    "release": "standard-version --no-verify",
    "release:alpha": "standard-version --prerelease alpha",
    "release:first": "standard-version --first-release",
    "prepublishOnly": "npm test && npm run build",
    "postpublish": "git push",
    "clean": "rm -rf ./lib/*",
    "prebuild": "npm run clean",
    "build:es": "ttsc -p build/tsconfig.es.json && npm run copyfile:es:js && npm run copyfile:es:dts",
    "build:lib": "ttsc -p build/tsconfig.lib.json && npm run copyfile:lib:js && npm run copyfile:lib:dts",
    "build": "npm run build:lib && npm run build:es",
    "watch": "NODE_ENV=development ttsc -p build/tsconfig.lib.json -w",
    "copyfile:es:js": "copyfiles -e \"@(build|es|lib)/**/*.js\" **/*.js es",
    "copyfile:es:dts": "copyfiles -e \"@(build|es|lib)/**/*.d.ts\" **/*.d.ts es",
    "copyfile:lib:js": "copyfiles -e \"@(build|es|lib)/**/*.js\" **/*.js lib",
    "copyfile:lib:dts": "copyfiles -e \"@(build|es|lib)/**/*.d.ts\" **/*.d.ts lib"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/sinon": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "ava": "^3.10.1",
    "commitizen": "^4.1.2",
    "copyfiles": "^2.3.0",
    "coveralls": "^3.1.0",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.13.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "mili": "^3.9.3",
    "nyc": "^15.1.0",
    "sinon": "^9.2.1",
    "standard-version": "^9.0.0",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "ttypescript": "^1.5.10",
    "typescript": "^4.0.5",
    "typescript-transform-paths": "^1.1.15"
  },
  "dependencies": {
    "@types/node": "^14.14.7",
    "blob": "^0.1.0",
    "clone": "^2.1.2",
    "cross-fetch": "^3.0.6",
    "formidable": "^2.0.0-canary.20200226.1",
    "mime-types": "^2.1.27",
    "nanoid": "^3.1.10",
    "path-to-regexp": "^6.2.0",
    "querystring": "^0.2.0",
    "url": "^0.11.0"
  },
  "description": "Request API write by Typescript for flexibility, readability, and a low learning curve.",
  "directories": {
    "doc": "doc",
    "lib": "lib",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Val-istar-Guo/keq.git"
  },
  "keywords": [
    "request",
    "superagent"
  ],
  "author": "Val.istar.Guo <val.istar.guo@gmail.com>",
  "bugs": {
    "url": "https://github.com/Val-istar-Guo/keq/issues"
  },
  "homepage": "https://github.com/Val-istar-Guo/keq#readme"
}
