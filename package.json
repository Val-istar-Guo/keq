{
  "name": "keq",
  "main": "lib/index.js",
  "module": "es/index.js",
  "version": "0.0.3",
  "license": "MIT",
  "scripts": {
    "test": "ava",
    "test:debug": "ava --verbose",
    "test:update": "ava -u",
    "report": "nyc --reporter=html --reporter=text-summary --skip-full ava",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "ci": "git-cz",
    "ca": "git add -A && git-cz -av",
    "release": "standard-version --no-verify",
    "release:alpha": "standard-version --prerelease alpha",
    "release:first": "standard-version --first-release",
    "prepublishOnly": "npm test && npm run build",
    "postpublish": "git push",
    "clean": "rm -rf ./lib/*",
    "prebuild": "npm run clean",
    "build:es": "ttsc -p build/tsconfig.es.json",
    "build:lib": "ttsc -p build/tsconfig.lib.json",
    "build": "npm run build:lib && npm run build:es",
    "watch": "NODE_ENV=development ttsc -p build/tsconfig.lib.json -w"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/sinon": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "ava": "^3.10.1",
    "commitizen": "^4.1.2",
    "coveralls": "^3.1.0",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.4.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "nyc": "^15.1.0",
    "sinon": "^9.0.2",
    "standard-version": "^8.0.2",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "ttypescript": "^1.5.10",
    "typescript": "^3.9.6",
    "typescript-transform-paths": "^1.1.14"
  },
  "dependencies": {
    "@types/node": "^14.0.23",
    "blob": "^0.1.0",
    "clone": "^2.1.2",
    "cross-fetch": "^3.0.5",
    "formidable": "^2.0.0-canary.20200226.1",
    "mime-types": "^2.1.27",
    "nanoid": "^3.1.10",
    "querystring": "^0.2.0",
    "url": "^0.11.0"
  }
}
